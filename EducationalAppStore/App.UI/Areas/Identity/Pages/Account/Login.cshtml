<!-- login section -->
<section class="forgot_pass">
	<div class="container">
		<div class="row">
			<div class="col-12 col-sm-7 col-sm-7 col-md-7 col-lg-7 mx-auto">
				<div class="forgot_wrapper">
					<h6>You login using your username or email address </h6>	
					<form method="post">
                        <div asp-validation-summary="ModelOnly" style="color:red"></div>
						<div class="form-group"> 
						  	<input autocomplete="off" class="required form-control" placeholder="Username or email" type="text" asp-for="Input.Email">
						<span style="color:red" asp-validation-for="Input.Email"></span>
                    </div>
                        

                        <div class="form-group"> 
						  	<input autocomplete="off" class="required form-control" placeholder="Password" type="password" asp-for="Input.Password">
                        <span style="color:red" asp-validation-for="Input.Password"></span>
						</div>
                         <div class="form-group"> 

                        <div class="container">
                          <div class="row">
                            <div class="col-8">
                              <label asp-for="Input.RememberMe"></label>
                              <input type="checkbox" id="lable" asp-for="Input.RememberMe"/>
                            </div>
                            <div class="col-4">
                              <a href="/identity/account/forgotpassword">forget password?</a>
                            </div>
                          </div>
                        </div>
                        </div>
						<div class="form-group register-btn">
						 	<button type="submit" class="btn btn-primary reset_pass_btn" >Login</button>
						</div>	
					</form>
				</div>
			</div>												
		</div>
	</div>
</section>
